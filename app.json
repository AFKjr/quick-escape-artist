{
  "expo": {
    "name": "Quick Escape Artist",
    "slug": "quick-escape-artist",
    "scheme": "qea",
    "icon": "./assets/images/splash-icon.png",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5E050"
    },
    "web": {
      "favicon": "./assets/images/splash-icon.png"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.studioafk.quickescapeartist",
      "infoPlist": {
        "UIBackgroundModes": ["audio"],
        "NSUserNotificationUsageDescription": "Allows urgent texts for graceful exits."
      }
    },
    "android": {
      "package": "com.studioafk.quickescapeartist",
      "permissions": [],
      "useNextNotificationsApi": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon.png",
        "backgroundColor": "#F5E050"
      }
    },
    "plugins": ["expo-notifications"],
    "assetBundlePatterns": ["**/*"]
  }
}